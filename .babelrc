{
    "plugins": [
        [
            "module-resolver", {
            "root": ["."],
            "alias": {
                "styles": "./styles"
            },
            "cwd": "babelrc"
        }],
        [
            "wrap-in-js",
            {
                "extensions": ["css$", "scss$"]
            }
        ],
        "transform-flow-strip-types",
        ["transform-define", "./env-config.js"]
    ],
    "presets": [
        "next/babel"
    ],
    "ignore": []
}